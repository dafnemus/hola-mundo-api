{
  "name": "hola-mundo-api",
  "version": "1.0.0",
  "description": "Api hola mundo GraphQL",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dafnemus/hola-mundo-api.git"
  },
  "keywords": [
    "graphql"
  ],
  "author": "Dafne Musante <dafnemus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dafnemus/hola-mundo-api/issues"
  },
  "homepage": "https://github.com/dafnemus/hola-mundo-api#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "graphql-import-node": "^0.0.5",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-tools": "^8.2.13",
    "http": "^0.0.1-security",
    "ncp": "^2.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.0.0",
    "nodemon": "^2.0.18"
  }
}
